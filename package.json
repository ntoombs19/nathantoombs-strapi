    "@strapi/plugin-seo": "^1.9.6",
    "@strapi/plugin-users-permissions": "4.15.0",
    "@strapi/provider-upload-aws-s3": "^4.15.0",
    "@strapi/strapi": "4.15.0",
    "pg": "^8.11.3",
    "strapi-plugin-sitemap": "^3.0.5",
    "strapi-plugin-slugify": "^2.3.6"
  },
  "author": {
    "name": "A Strapi developer"
  },
  "strapi": {
